{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "env": {
        "PLAYWRIGHT_BROWSER": "chromium"
      }
    }
  },
  "browsers": {
    "headless": false,
    "viewport": {
      "width": 1280,
      "height": 720
    }
  },
  "allowedOrigins": [
    "http://localhost:3000",
    "https://amazonanalyst.ai"
  ]
}